<%= form_for(:session, url: login_path ) do |f| %>
 
  <div class="field">
    <%= f.label :cuenta %><br>
    <%= f.text_field :cuenta, class: 'form-control' %>
  </div>
  <div class="field">
    <%= f.label :password %><br>
    <%= f.password_field :password, class: 'form-control' %>
  </div>
  <div class="actions">
    <%= f.submit "Log in", class: "btn btn-primary"%>
  </div>
<% end %>
